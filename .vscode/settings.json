{
	"window.commandCenter": true,
	"explorer.compactFolders": true,
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "vscode-icons",
    "redhat.telemetry.enabled": true,
	"git.decorations.enabled": false,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"git.autofetch": true,
	"ShortcutMenuBar.beautify": false,
	"ShortcutMenuBar.openFilesList": false,
	"ShortcutMenuBar.redo": true,
	"ShortcutMenuBar.toggleActivityBar": true,
	"ShortcutMenuBar.navigateBack": false,
	"ShortcutMenuBar.navigateForward": false,
	"ShortcutMenuBar.userButton04Command": "workbench.action.reopenClosedEditor",
	"ShortcutMenuBar.userButton06Command": "extension.scenebuilder.open-in",
	"ShortcutMenuBar.userButton09Command": "commitMsg.autofill, git.commitAll, git.sync",
	"gitlens.graph.minimap.dataType": "lines",
	"gitlens.graph.minimap.additionalTypes": [
		"localBranches",
		"stashes",
		"remoteBranches",
		"tags"
	],
	"todo-tree.regex.regex": "(//|#|<!--|/\\*|^\\s*\\*)\\s*($TAGS)",
	"todo-tree.filtering.excludeGlobs": [
		"**/vendor/**",
        "**/node_modules/**",
        "**/dist/**",
        "**/bower_components/**",
        "**/build/**",
        "**/.vscode/**",
        "**/.github/**",
        "**/_output/**",
        "**/*.min.*",
        "**/*.map"
	],
	"java.compile.nullAnalysis.mode": "automatic",

	"java.project.referencedLibraries": [
		"target/classes",
		"lib/**/*.jar"
	],
	"java.configuration.runtimes": [],
	"java.dependency.packagePresentation": "hierarchical",
	"java.dependency.showMembers": true,
	"java.project.exportJar.targetPath": "${workspaceFolder}/out/artifacts/${workspaceFolderBasename}_jar/${workspaceFolderBasename}.jar",
	"java.completion.filteredTypes": [
		"com.sun.*",
		"sun.*",
		"jdk.*",
		"org.graalvm.*",
		"io.micrometer.shaded.*"
	],
	"java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx2G -Xms100m -Xlog:disable",
	"java.configuration.updateBuildConfiguration": "automatic",
	"terminal.integrated.scrollback": 1e+69,
	"maven.view": "hierarchical",

	"java.format.settings.url": ".vscode\\custom-eclipse-java-google-style.xml",
	"[java]": {
		"editor.defaultFormatter": "redhat.java",
		"editor.indentSize": "tabSize",
		"editor.insertSpaces": false,
		"editor.wordWrap": "off"

	},
	"files.watcherExclude": {
		"**/target": true
	},
	"java.maven.downloadSources": true,
	"java.import.maven.offline.enabled": false,
	"java.eclipse.downloadSources": true,
	"java.project.sourcePaths": [
		"src\\main\\scala"
		],
	"[scala]": {
	"editor.defaultFormatter": "scalameta.metals"
	}
	}